<ContentControl x:Class="HGM.Hotbird64.Icons.QueryKey"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HGM.Hotbird64.Icons"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Viewbox>
        <Canvas Width="100" Height="100">
            <local:QuestionOrange Width="100"/>
            <local:KeyIcon Width="75">
                <local:KeyIcon.RenderTransform>
                    <RotateTransform Angle="135" CenterX="40" CenterY="49"></RotateTransform>
                </local:KeyIcon.RenderTransform>
            </local:KeyIcon>
        </Canvas>
    </Viewbox>
</ContentControl>
