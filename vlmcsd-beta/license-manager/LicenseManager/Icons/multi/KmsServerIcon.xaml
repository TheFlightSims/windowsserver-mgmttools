<ContentControl x:Class="HGM.Hotbird64.Icons.KmsServerIcon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:icons="clr-namespace:HGM.Hotbird64.Icons"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
  <Viewbox>
    <Grid>
         <icons:Server/>
      <icons:KeyIcon Angle="135" Width="700">
        <icons:KeyIcon.RenderTransform>
          <TranslateTransform X="100" Y="135"></TranslateTransform>
        </icons:KeyIcon.RenderTransform>
        </icons:KeyIcon>
    </Grid>
  </Viewbox>
</ContentControl>
